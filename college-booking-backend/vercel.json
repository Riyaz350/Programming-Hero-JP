{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/health",
        "dest": "api/index.js",
        "methods": ["GET"]
      },
      {
        "src": "/api/v1/(.*)",
        "dest": "src/index.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Headers": "Content-Type, Authorization",
          "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH, OPTIONS"
        }
      },
      {
        "src": "/(.*)",
        "dest": "api/index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
}
  